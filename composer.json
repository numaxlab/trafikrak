{
    "name": "numaxlab/trafikrak",
    "type": "library",
    "version": "1.0.0-beta.1",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "intervention/validation": "^4.6",
        "numaxlab/lunar-geslib": "*",
        "spatie/laravel-translatable": "^6.0",
        "filament/spatie-laravel-translatable-plugin": "^3.3",
        "lunarphp/table-rate-shipping": "^1.0",
        "lunarphp/meilisearch": "^1.0",
        "meilisearch/meilisearch-php": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-livewire": "3.x-dev",
        "orchestra/testbench": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Trafikrak\\": "src/",
            "Trafikrak\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trafikrak\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trafikrak\\TrafikrakServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Adri√°n Pardellas Blunier",
            "email": "adrian@numax.org"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
